<html>
    <head>
        <link rel="stylesheet" href="/css/index.css">
    </head>
    <title>
        {{ .title }}
    </title>
    <body>
        <form action="/upload" method="post" enctype="multipart/form-data">
            <input type="file" id="file" name="filename">
            <input type="submit">
        </form>
        <div class="gallery">
            {{range .images}}
                <img class="image" src="{{.Thumbnail.Path}}">
            {{end}}
        </div>
    </body>
</html>